
.box {
	position: relative;
}
.back {
	width: 80rpx;
	height: 80rpx;
	background-color: rgba(0, 0, 0, 0.3);
	border-radius: 50%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	position: absolute;
	left: 10rpx;
	top: 10rpx;
	z-index: 5;
}
